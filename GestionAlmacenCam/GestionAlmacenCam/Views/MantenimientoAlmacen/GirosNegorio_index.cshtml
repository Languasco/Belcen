<div class=" container" ng-init="initAll();">
    <div class="panel-body ContentFilters contentCombo" style=" margin-bottom: -35px;">
        <div class="well" style="background-color: #2b4c6c;padding-bottom: 0px; padding-top: 0px;color: #fff;    width: 100%;">
            <div class="row">
                <div class="col-sm-12 text-center ">

                    <div class="row col-sm-3">
                        <div style="width: 80%">
                            <label for="cbo_ESTADO">ESTADO</label> <span class="help-block"></span>

                            <select class="select_modal" id="cboestadoFilter" ng-model="estados" data-style="btn-default" ng-change="changeEstado(estados)" style=" font-size: 15px;   color: #0e0e0e;">
                                <option ng-repeat="item in listStatus" value="{{item.id}}">{{item.descripcion}}</option>
                            </select>
                        </div>
                    </div>
                    <div class="row col-sm-6">
                        <div class="form-group" style="margin-top: 15px">
                            <div class="input-group input-group-sm">
                                <span class="input-group-addon">Buscar</span>
                                <input id="inputSearch" type="text" placeholder="Busqueda General . . " style="width: 120%;" class="form-control btn-block inputSearch" autocomplete="off" @*onkeyup="this.value=this.value.toUpperCase();"*@>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12 " style="margin-bottom: 15px;margin-top: -16px;">

                    <div class="btn-group">
                        <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click="Open_New_Modal();">
                            <i class="glyphicon glyphicon-plus"></i>Agregar Nuevo
                        </button>
                        <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click=" ExportarToExcel();">
                            <i class="glyphicon glyphicon-save"></i>Exportar
                        </button>
                        <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click=" open_new_modal();">
                            <i class="glyphicon glyphicon-floppy-disk"></i>Importar
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="panel-body ContentFilters">
        <div class="form-inline ">
            <div class="row">
                <div class="col-sm-12">
                    <div style="text-align:center">
                        <div class="cssload-speeding-wheel" ng-if="loader"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="white-box" style="border-left: 20px solid #2b4c6c;">
                    <table id="tbl_girosN" class="table m-b-0 toggle-arrow-tiny table-bordered table-hover" data-page-size="8">
                        <thead>
                            <tr>
                                <th data-toggle="true">#</th>
                                <th data-toggle="true" width="30%">NOMBRE GIRONEGOCIO</th>
                                <th data-toggle="true">ABREVIATURA GIRONEGOCIO</th>
                                <th data-toggle="true" width="30%">DESCRIPCION GIRONEGOCIO</th>
                                <th data-toggle="true"> ESTADO></th>
                                <th>Acciones</th>
                            </tr>
                        </thead>

                        <tbody>
                            <tr ng-repeat="item in ListandoGirosN" style="{{item.estado == 0 | iif : 'background-color: rgb(230, 115, 115) !important;color:#fff;font-weight: 600;'}}">
                                <td>{{$index + 1}}</td>
                                <td width="30%">{{item.Nombre_GiroNegocio}}</td>
                                <td>{{item.abreviatura_GiroNegocio}}</td>
                                <td width="30%">{{item.descripcion_GiroNegocio}}</td>
                                <td>{{item.estado == 0 | iif : "Anulado" : "Activado"}}</td>
                                <td>

                                    <button type="button" class="btn btn-primary btn-circle " ng-click="Open_Update_Modal(item);" data-toggle="tooltip" title="EDITAR"><i class=" ti-pencil-alt"></i></button>
                                    <button type="button" class="btn btn-success btn-circle" ng-click="getAuditoria(item)"><i class="fa fa-cog"></i> </button>
                                    <button type="button" class="btn btn-warning btn-circle" ng-click="getAnular(item);" style="background-color: #ff6849; border-color: #ff6849;" title="ANULAR"><i class="fa fa-trash"></i> </button>

                                </td>
                            </tr>
                        </tbody>
                    </table>

                    <div class="text-right">
                        <ul class="pagination pagination-split m-t-30"></ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="modalGiroNegocio" class="modal fade bd-example-modal-lg " tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background:#398ad8; padding: 15px; border-bottom: 2px solid #f71166; color:#ffffff">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myLargeModalLabel" style="color:#ffffff !important">{{titleModal}}</h4>
            </div>
            <div class="modal-body">

                <div class="loaderModal" ng-if="loaderSave"></div>

                <div class="contentRegistro floating-labels form-inline">
                    <fieldset>
                        <legend style="text-align:center !important">INGRESO DE DATOS GENERALES DE GIROS DE NEGOCIO</legend>
                        <div class="row">
                            <div class="col-md-6">
                                <input type="text" class="form-control" id="txtNombre_GN" ng-model="objeto_parametros.Nombre_GiroNegocio" ng-keypress="Validationletter($event);" ng-keydown="ValidateEnterGN($event);" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                <label for="txtNombre_GN">NOMBRE DE GIRO_NEGOCIO</label> <span class="help-block"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-md-8">
                                <input type="text" class="form-control" id="txtAbrevi_GN" ng-model=" objeto_parametros.abreviatura_GiroNegocio" ng-keydown="ValidateEnterN($event);" ng-keypress="Validationletter($event);" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                <label for="txtAbrevi_GN">ABREVIATURA DE GIRO_NEGOCIO</label> <span class="help-block"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <input type="text" class="form-control" id="txtdescr_GN" ng-model="objeto_parametros.descripcion_GiroNegocio" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                <label for="txtdescr_GN">DESCRIPCION DE GIRO_NEGOCIO</label> <span class="help-block"></span>
                            </div>
                        </div>
                    </fieldset>
                </div>

                <div class="contentRegistro floating-labels form-inline">
                    <div>
                        <div class="row ">
                            <div class="col-sm-6">
                                <fieldset>
                                    <legend>Estado</legend>
                                    <div class="row " style=" margin-top: -10px;">
                                        <div class="col-sm-6 ">
                                            <div class="form-group">
                                                <div class="checkbox checkbox-success">
                                                    <input id="checkbox1" type="checkbox" ng-model="objEstados.activo" ng-change="changeStatus(objEstados.activo);">
                                                    <label for="checkbox1" class="estadoModal" style="color : {{objEstados.colorText}}">{{objEstados.text}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer ">
                <a class="btn btn-primary" ng-click="GuardarRegistro();" id="btn-save"> <span class="glyphicon glyphicon-floppy-disk"></span> Guardar</a>
                <a class="btn btn-danger" data-dismiss="modal" id="btn-close"><span class="glyphicon glyphicon-remove-sign"></span> Cerrar</a>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
