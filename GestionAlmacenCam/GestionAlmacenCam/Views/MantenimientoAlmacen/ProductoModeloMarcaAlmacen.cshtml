<style>
    #listMenu span, a {
        text-align: left !important;
    }

    .contentRegistro fieldset legend {
        text-align: left !important;
    }

    .jq-toast-wrap {
        width: 300px !important;
    }

    .floating-labels label {
        top: -6px;
    }

    .contentRegistro .select2-container {
        margin-left: 0px;
        margin-top: 2px;
    }

    #paddin-lef {
        padding-left: 40px !important;
    }

    #btn-material {
        font-size: 1.2rem;
        outline: 0;
        border: none;
        border-radius: 2px;
        display: inline-block;
        height: 36px;
        line-height: 36px;
        padding: 0 2rem;
        text-transform: uppercase;
        vertical-align: middle;
        -webkit-tap-highlight-color: transparent;
        text-decoration: none;
        color: #fff;
        background-color: #2b4c6c;
        text-align: center;
        letter-spacing: .5px;
        -webkit-transition: .2s ease-out;
        transition: .2s ease-out;
        cursor: pointer;
        -webkit-box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
        box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
    }

    #btn-save {
        font-size: 1.2rem;
        outline: 0;
        border: none;
        border-radius: 2px;
        border: 1px solid #2c5ca9;
        display: inline-block;
        height: 36px;
        line-height: 36px;
        padding: 0 2rem;
        text-transform: uppercase;
        vertical-align: middle;
        -webkit-tap-highlight-color: transparent;
        text-decoration: none;
        color: #fff;
        background: #2c5ca9;
        text-align: center;
        letter-spacing: .5px;
        -webkit-transition: .2s ease-out;
        transition: .2s ease-out;
        cursor: pointer;
        -webkit-box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
        box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
    }

    #btn-close {
        font-size: 1.2rem;
        outline: 0;
        border: none;
        border-radius: 2px;
        background: #f75b36;
        border: 1px solid #f75b36;
        display: inline-block;
        height: 36px;
        line-height: 36px;
        padding: 0 2rem;
        text-transform: uppercase;
        vertical-align: middle;
        -webkit-tap-highlight-color: transparent;
        text-decoration: none;
        color: #fff;
        text-align: center;
        letter-spacing: .5px;
        -webkit-transition: .2s ease-out;
        transition: .2s ease-out;
        cursor: pointer;
        -webkit-box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
        box-shadow: 0 2px 2px 0 rgba(0,0,0,0.14), 0 1px 5px 0 rgba(0,0,0,0.12), 0 3px 1px -2px rgba(0,0,0,0.2);
    }

    .contentCombo .select2-container {
        margin-left: 0px;
        margin-top: -20px;
        /*width: 120% !important;*/
    }

    .ContentFilters .table {
        display: table;
    }
</style>

<div class=" container" ng-init="initAll();">
    <div class="panel-body ContentFilters">
        <div class="form-inline ">
            <div class="row">
                <div class="col-sm-12">
                    <div style="text-align:center">
                        <div class="cssload-speeding-wheel" ng-if="loader"></div>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel-body ContentFilters contentCombo" style=" margin-bottom: -35px;">
            <div class="well" style="background-color: #2b4c6c;padding-bottom: 0px; padding-top: 0px;color: #fff;    width: 100%;">
                <div class="row">
                    <div class="col-sm-12 text-center ">

                        <div class="row col-sm-3">
                            <div style="width: 80%">
                                <label for="cbo_ESTADO">ESTADO</label> <span class="help-block"></span>

                                <select class="select_modal" id="cboestadoFilter" ng-model="estados" data-style="btn-default" ng-change="changeEstado(estados)" style=" font-size: 15px;   color: #0e0e0e;">
                                    <option ng-repeat="item in listStatus" value="{{item.id}}">{{item.descripcion}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="row col-sm-6">
                            <div class="form-group" style="margin-top: 15px">
                                <div class="input-group input-group-sm">
                                    <span class="input-group-addon">Buscar</span>
                                    <input id="inputSearch" type="text" placeholder="Busqueda General . . " style="width: 120%;" class="form-control btn-block inputSearch" autocomplete="off" @*onkeyup="this.value=this.value.toUpperCase();"*@>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-12 " style="margin-bottom: 15px;margin-top: -16px;">

                        <div class="btn-group">
                            <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click="Open_New_Modal();">
                                <i class="glyphicon glyphicon-plus"></i>Agregar Nuevo
                            </button>
                            <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click=" ExportarToExcel();">
                                <i class="glyphicon glyphicon-save"></i>Exportar
                            </button>
                            <button id="btn-material" style="background-color: #357c55;" class="btn btn-outline btn-primary btn-sm m-l-10" ng-click=" open_new_modal();">
                                <i class="glyphicon glyphicon-floppy-disk"></i>Importar
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12">
                <div class="white-box" style="border-left: 20px solid #2b4c6c;">
                    <table id="tblProductoModelo" class="table m-b-0 toggle-arrow-tiny table-bordered table-hover" data-page-size="10">
                        <thead>
                            <tr>
                                <th data-toggle="true">#</th>
                                <th data-toggle="true" id="paddin-lef">NombreMarcaProducto</th>
                                <th data-toggle="true" id="paddin-lef">CodigoModeloProd</th>
                                <th data-toggle="true" id="paddin-lef">NombreModeloProd</th>
                                <th data-toggle="true" id="paddin-lef">AbreviaturaModeloProd</th>
                                <th data-toggle="true"> ESTADO</th>
                                <th id="paddin-lef">Acciones</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="item in listProModelo" style="{{item.estado == 0 | iif : 'background-color: rgb(230, 115, 115) !important;color:#fff;font-weight: 600;'}}">

                                <td>{{$index + 1}}</td>
                                <td id="paddin-lef">{{item.nombre_marcaproducto}}</td>
                                <td id="paddin-lef">{{item.codigo_modeloProducto}}</td>
                                <td id="paddin-lef">{{item.nombre_modeloProducto}}</td>
                                <td id="paddin-lef">{{item.abreviatura_modeloProducto}}</td>
                                <td>{{item.estado == 0 | iif : "Anulado" : "Activado"}}</td>
                                <td id="">

                                    <button type="button" class="btn btn-primary btn-circle " ng-click="Open_Update_Modal(item);" data-toggle="tooltip" title="EDITAR"><i class=" ti-pencil-alt"></i></button>
                                    <button type="button" class="btn btn-success btn-circle" ng-click="getAuditoria(item)"><i class="fa fa-cog"></i> </button>
                                    <button type="button" class="btn btn-warning btn-circle" ng-click="getAnular(item);" style="background-color: #ff6849; border-color: #ff6849;"><i class="ti-trash"></i></button>
                                </td>
                            </tr>
                        </tbody>
                    </table>

                    <div class="text-center">
                        <ul class="pagination pagination-split m-t-30"></ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="ModalModeloProducto" class="modal fade bd-example-modal-lg " tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false" aria-labelledby="myLargeModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background:#009DC3;  padding: 15px; border-bottom: 2px solid #f71166; color:#ffffff">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                <h4 class="modal-title" id="myLargeModalLabel" style="color: #fcfdff !important;">{{titleModal}}</h4>
            </div>
            <div class="modal-body">

                <div class="loaderModal" ng-if="loaderSave"></div>

                <div class="contentRegistro floating-labels form-inline">
                    <fieldset>
                        <legend>INGRESO DE DATOS DE PRODUCTO MODELO MARCA</legend>
                        <div class="row">
                            <div class="col-md-8">
                                <label for="cbo_vehiculo" style="top:-5px !important; color: #f71333; font-weight: 600; letter-spacing: 1px;">MARCA DEL PRODUCTO</label> <span class="help-block"></span>
                                <select class="selectModal" id="combMarca" ng-model="objProductoModeloMarca.id_marcaProducto" ng-change="changeSelect('marca',objProductoModeloMarca.id_marcaProducto)" data-width="35%" data-style="btn-default">
                                    <option value="0">     ------Seleccionar ----- </option>
                                    <option ng-repeat="item in listProductoMarca" value="{{item.id_marcaProducto}}">{{item.nombre_marcaproducto}}</option>
                                </select>
                            </div>
                        </div>
                        <br />
                        <div class="row">
                            <div class="form-group col-sm-6">
                                <input type="text" class="form-control" id="codigointerno" required ng-model="objProductoModeloMarca.codigo_modeloProducto" maxlength="10" ng-keypress="ValidationNumber($event);" ng-keydown="ValidateEnterN($event);" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                <label for="codigointerno">CODIGO DE MODELO MARCA</label> <span class="help-block"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-sm-6">
                                <input type="text" class="form-control" id="txtnombre" required ng-model="objProductoModeloMarca.nombre_modeloProducto" maxlength="20" ng-keydown="ValidateEnterDes($event);" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                <label for="txtnombre">NOMBRE DE MODELO MARCA</label> <span class="help-block"></span>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12">
                                <div class="form-group col-sm-10">
                                    <input type="text" class="form-control" id="txtdes_m" required ng-model="objProductoModeloMarca.abreviatura_modeloProducto" maxlength="40" onkeyup="this.value=this.value.toUpperCase();"><span class="highlight"></span> <span class="bar"></span>
                                    <label for="txtdes_m">ABREVIATURA DE MODELO MARCA</label> <span class="help-block"></span>
                                </div>
                            </div>
                        </div>

                    </fieldset>
                </div>

                <div class="contentRegistro floating-labels form-inline">
                    <div>
                        <div class="row ">
                            <div class="col-sm-6">
                                <fieldset>
                                    <legend>Estado</legend>
                                    <div class="row " style=" margin-top: -10px;">
                                        <div class="col-sm-6 ">
                                            <div class="form-group">
                                                <div class="checkbox checkbox-success">
                                                    <input id="checkbox1" type="checkbox" ng-model="objEstados.activo" ng-change="changeStatus(objEstados.activo);">
                                                    <label for="checkbox1" class="estadoModal" style="color : {{objEstados.colorText}}">{{objEstados.text}}</label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div class="modal-footer ">
                <a class="btn btn-primary" ng-click="saveProductoModeloMarca();" id="btn-save"> <span class="glyphicon glyphicon-floppy-disk"></span> Guardar</a>
                <a class="btn btn-danger" data-dismiss="modal" id="btn-close"><span class="glyphicon glyphicon-remove-sign"></span> Cerrar</a>
            </div>
        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>







